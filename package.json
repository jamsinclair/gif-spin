{
  "name": "gif-spin",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "xo --fix",
    "lint": "xo"
  },
  "dependencies": {
    "ascender": "^0.5.0",
    "file-saver": "^2.0.5",
    "gifski-wasm": "^2.0.2",
    "preact": "^10.22.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.2",
    "eslint-config-preact": "^1.4.0",
    "vite": "^5.0.0",
    "xo": "^0.58.0"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "xo": {
    "prettier": true,
    "extends": "preact",
    "rules": {
      "import/extensions": "off",
      "unicorn/filename-case": "off",
      "jest/no-deprecated-functions": "off",
      "node/file-extension-in-import": "off",
      "node/prefer-global/process": "off"
    }
  },
  "alias": {
    "preact/jsx-dev-runtime": "preact/compat/jsx-dev-runtime"
  }
}